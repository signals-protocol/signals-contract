{
  "name": "signals-temp-contract",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-ignition": "^0.15.0",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@openzeppelin/contracts": "^5.2.0",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": ">=9.1.0",
    "@types/node": ">=18.0.0",
    "chai": "^4.2.0",
    "dotenv": "^16.4.7",
    "ethers": "^6.4.0",
    "hardhat": "^2.22.19",
    "hardhat-gas-reporter": "^1.0.8",
    "solidity-coverage": "^0.8.0",
    "ts-node": ">=8.0.0",
    "typechain": "^8.3.0",
    "typescript": ">=4.5.0"
  },
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:gas": "REPORT_GAS=true hardhat test",
    "test:market": "hardhat test --grep \"Market Creation\"",
    "test:token": "hardhat test --grep \"Buying Tokens\"",
    "test:math": "hardhat test --grep \"formula\"",
    "test:claim": "hardhat test --grep \"Claiming Rewards\"",
    "coverage": "hardhat coverage",
    "node": "hardhat node",
    "deploy:local": "hardhat run scripts/deploy.ts --network localhost",
    "deploy:rsk": "hardhat run scripts/deploy.ts --network rskTestnet",
    "deploy:amoy": "hardhat run scripts/deploy.ts --network polygonAmoy",
    "deploy:citrea": "hardhat run scripts/deploy.ts --network citreaTestnet",
    "interact:local": "hardhat run scripts/interact.ts --network localhost",
    "interact:rsk": "hardhat run scripts/interact.ts --network rskTestnet",
    "interact:amoy": "hardhat run scripts/interact.ts --network polygonAmoy",
    "interact:citrea": "hardhat run scripts/interact.ts --network citreaTestnet",
    "create-markets:local": "hardhat run scripts/dev/createMultipleMarkets.ts --network localhost",
    "create-markets:rsk": "hardhat run scripts/dev/createMultipleMarkets.ts --network rskTestnet",
    "create-markets:amoy": "hardhat run scripts/dev/createMultipleMarkets.ts --network polygonAmoy",
    "create-markets:citrea": "hardhat run scripts/dev/createMultipleMarkets.ts --network citreaTestnet",
    "bet:local": "hardhat run scripts/dev/betMultipleMarkets.ts --network localhost",
    "bet:rsk": "hardhat run scripts/dev/betMultipleMarkets.ts --network rskTestnet",
    "bet:amoy": "hardhat run scripts/dev/betMultipleMarkets.ts --network polygonAmoy",
    "bet:citrea": "hardhat run scripts/dev/betMultipleMarkets.ts --network citreaTestnet",
    "check-balance:amoy": "hardhat run scripts/checkBalance.ts --network polygonAmoy",
    "check-balance:citrea": "hardhat run scripts/checkBalance.ts --network citreaTestnet",
    "clean": "hardhat clean",
    "format": "prettier --write \"contracts/**/*.sol\" \"test/**/*.ts\" \"scripts/**/*.ts\"",
    "lint": "solhint \"contracts/**/*.sol\""
  },
  "dependencies": {
    "@prb/math": "^4.1.0"
  }
}
